{
  "executable": "TreeMage",
  "splashscreen": {
    "imageFile": ""
  },
  "name": "TreeMage",
  "author": "Funny_Silkie",
  "singleInstance": false,
  "environment": "Production",
  "build": {
    "appId": "com.TreeMage.app",
    "productName": "TreeMage",
    "copyright": "Copyright Â© 2024",
    "buildVersion": "1.1.0-dev",
    "compression": "maximum",
    "win": {
      "icon": "../../../Assets/icon.png"
    },
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [
          "**/*"
        ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [
          "**/*"
        ]
      },
      "**/*"
    ]
  }
}
